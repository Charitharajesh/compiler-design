#include <stdio.h>
#include <string.h>

char s[10];
int i = 0;

void E() {
    if (s[i]=='i' && s[i+1]=='d') {
        i += 2;
        if (s[i]=='+') {
            i++;
            if (s[i]=='i' && s[i+1]=='d')
                i += 2;
        }
    }
}

int main() {
    printf("Enter input: ");
    scanf("%s", s);
    E();

    if (i == strlen(s))
        printf("String Accepted\n");
    else
        printf("String Rejected\n");
    return 0;
}


sample output
String Accepted
